{
  "workbench.colorCustomizations": {
    "titleBar.activeForeground": "#100030",
    "titleBar.inactiveForeground": "#100030",
    "titleBar.activeBackground": "#f050f8",
    "titleBar.inactiveBackground": "#f050f8",
    "statusBar.background": "#f050f8",
    "statusBar.foreground": "#100030"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "stylelint.enable": true,
  "css.validate": false,
  "scss.validate": false,
  "stylelint.validate": ["scss", ".module.scss"],
  "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
